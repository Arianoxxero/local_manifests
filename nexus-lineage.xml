<?xml version="1.0" encoding="UTF-8"?>
<!-- Manifest for LineageOS and LineageOS based roms-->
<manifest>

	<!-- Device Trees -->
	<project path="device/samsung/zero-common" name="TeamNexus/android_device_samsung_zero-common" />
	<project path="device/samsung/zerofltexx" name="TeamNexus/android_device_samsung_zerofltexx" />
	<project path="device/samsung/zeroltexx" name="TeamNexus/android_device_samsung_zeroltexx" />
	<project path="device/samsung/zerofltecan" name="TeamNexus/android_device_samsung_zerofltecan" />
	<project path="device/samsung/zeroltecan" name="TeamNexus/android_device_samsung_zeroltecan" />

	<!-- Vendor folder -->
	<project path="vendor/nexus" name="TeamNexus/android_vendor_nexus" />
	<project path="vendor/samsung/zero-common" name="TeamNexus/android_vendor_samsung_zero-common" />

	<!-- Kernel -->
	<project path="kernel/samsung/exynos7420" name="TeamNexus/android_kernel_samsung_exynos7420" />

	<!-- Hardware -->
	<project path="hardware/samsung" name="LineageOS/android_hardware_samsung" />
	<project path="hardware/samsung_slsi-cm/exynos" name="LineageOS/android_hardware_samsung_slsi-cm_exynos" />
	<project path="hardware/samsung_slsi-cm/exynos5" name="LineageOS/android_hardware_samsung_slsi-cm_exynos5" />
	<project path="hardware/samsung_slsi-cm/openmax" name="LineageOS/android_hardware_samsung_slsi-cm_openmax" />
	<project path="hardware/samsung_slsi-cm/exynos7420" name="LineageOS/android_hardware_samsung_slsi-cm_exynos7420" />

	<!-- Application-Sources -->
	<project path="packages/apps/TheNexus" name="TeamNexus/android_packages_apps_TheNexus" />

	<!-- Magisk -->
	<project path="packages/magisk" name="TeamNexus/android_packages_magisk" />
	<project path="packages/magisk/jni/magiskpolicy" name="TeamNexus/android_packages_magisk_magiskpolicy" />
	<project path="packages/magisk/jni/ndk-compression" name="TeamNexus/android_packages_magisk_ndk-compression" />
	<project path="packages/magisk/jni/selinux" name="TeamNexus/android_packages_magisk_selinux" />
	<project path="packages/magisk/jni/su" name="TeamNexus/android_packages_magisk_su" />

	<!-- Etc -->
	<project path="external/stlport" name="LineageOS/android_external_stlport" />
	
	<!-- LineageOS Overrides -->
	<remove-project name="LineageOS/android_packages_apps_CMUpdater" />

</manifest>
